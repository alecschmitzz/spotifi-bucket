{
  "name": "spotifi-bucket",
  "module": "src/index.ts",
  "type": "module",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.474.0",
    "@aws-sdk/s3-request-presigner": "^3.474.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.18.2"
  },
  "scripts": {
    "dev": "dotenv -e .env.development -- bun --hot run ./src/index.ts",
